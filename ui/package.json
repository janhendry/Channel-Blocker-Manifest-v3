{
    "name": "channel-blocker-ui",
    "version": "1.0.0",
    "description": "The ui for the channel blocker extension.",
    "main": "index.js",
    "scripts": {
        "build-sass": "node-sass --include-path sass ./src/settings/style --output ./dist/settings/style",
        "build": "esbuild --bundle src/settings/scripts/index.ts --outfile=dist/settings/index.js && npm run build-sass"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Time-Machine-Development/Channel-Blocker.git"
    },
    "author": "Marc Beyer",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Time-Machine-Development/Channel-Blocker/issues"
    },
    "homepage": "https://github.com/Time-Machine-Development/Channel-Blocker#readme",
    "dependencies": {
        "core": "1.0.0"
    },
    "devDependencies": {
        "node-sass": "^9.0.0",
        "esbuild": "^0.23.0"
    }
}